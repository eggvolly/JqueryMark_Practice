@model List<JqueryMark.ViewModel.UserIdentityViewModel>

<table class="table table-hover">
    <tr>
        <th style="width:20%">Name</th>
        <th style="width:20%">DisplayName</th>
        <th style="width:60%">Description</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Name
            </td>
            <td>
                @item.DisplayName
            </td>
            <td>
                @item.Description
            </td>
        </tr>
    }
</table>